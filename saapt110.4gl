                   CALL cl_err3("upd","apa_file",g_apa_t.apa01,"",SQLCA.sqlcode,"","upd apa1:",1)  #No.FUN-660122
                   LET g_success='N'
                   IF p_cmd = 'u' THEN
                      LET g_apv[i].*=g_apv_t.*
                   END IF
                 ELSE
                    CALL t110_comp_oox1(g_apv[i].apv03,g_apv[i].apv05) RETURNING g_net  #MOD-940090                   	
