#若有維護多借方資料，在單頭調整後，需依據單頭匯率重算本幣金額
FUNCTION t110_chk_debit()
DEFINE l_cnt      LIKE type_file.num10
DEFINE l_api03    LIKE api_file.api03    #項次
DEFINE l_api05    LIKE api_file.api05    #本幣金額
DEFINE l_api05f   LIKE api_file.api05f   #原幣金額
DEFINE l_sql      STRING

   LET l_cnt = 0

   SELECT COUNT(*)
     INTO l_cnt
     FROM api_file
    WHERE api01 = g_apa.apa01

   IF cl_null(l_cnt) THEN
      LET l_cnt = 0
   END IF

   IF l_cnt > 0 THEN   #多借方有資料，依據單頭匯率重算金額
      LET l_sql = " SELECT api03,api05f FROM api_file ",
                  "  WHERE api01 = '",g_apa.apa01,"' ",
                  " ORDER BY api03 "
      PREPARE t110_pre_api05f FROM l_sql
      DECLARE t100_api05f CURSOR FOR t110_pre_api05f

      LET l_sql = " UPDATE api_file SET api05 = ? ",
                  "  WHERE api01 = ? ",
                  "    AND api03 = ? "
      PREPARE t110_upd_api05 FROM l_sql

      FOREACH t100_api05f INTO l_api03,l_api05f
         IF SQLCA.sqlcode THEN
            CALL cl_err('FOREACH t100_api05f:',SQLCA.sqlcode,1)
            LET g_success = 'N'
            EXIT FOREACH
         END IF
         LET l_api05 = l_api05f * g_apa.apa14
         LET l_api05 = cl_digcut(l_api05,g_azi04)

         EXECUTE t110_upd_api05 USING l_api05,g_apa.apa01,l_api03
         IF SQLCA.sqlcode THEN
            CALL cl_err('UPDATE api05:',SQLCA.sqlcode,1)
            LET g_success = 'N'
            EXIT FOREACH
         END IF
      END FOREACH
   END IF

END FUNCTION
