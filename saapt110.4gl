    #END IF                                                                     #210827 mark by ruby
 
   #-MOD-AB0117-add-
    #IF g_apa.apa00 = "13" AND (g_apa.apa34  - g_apa.apa35) = 0 THEN                          #No.TQC-B50108
    IF (g_apa.apa00 = "13" OR g_apa.apa00 = '17') AND (g_apa.apa34  - g_apa.apa35) = 0 THEN   #No.TQC-B50108
       RETURN
    END IF
   #-MOD-AB0117-end-
