                      "  FROM ",cl_get_target_table(g_plant_new,'apg_file'),",",
                                cl_get_target_table(g_plant_new,'apf_file'),
                      " WHERE apg01 = apf01 AND apf41='Y' ",
                      "   AND apg04 = '",g_apa.apa01,"'",
