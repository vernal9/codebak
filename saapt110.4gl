                 ON ACTION qbe_save
		   CALL cl_qbe_save()
      END CONSTRUCT
 
      IF INT_FLAG THEN
         LET INT_FLAG=0
         CLOSE WINDOW t110_w9
         RETURN
      END IF
   END IF
 
   CLOSE WINDOW t110_w9
 
   MESSAGE "WORKING !"
   LET g_success = 'Y'
