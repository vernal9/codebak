     END IF
     DISPLAY BY NAME g_apb[l_ac].b251
  END IF
  
  DISPLAY BY NAME g_apb[l_ac].apb35,g_apb[l_ac].apb36,g_apb[l_ac].apb31
END FUNCTION
 
