  IF g_aza.aza63 = 'Y' THEN
     IF g_apb[l_ac].apb251 <> g_apb_t.apb251 THEN
        LET l_cmd = 'a'
     END IF
  END IF
  #和apmt540類似,因為KEYIN時,可能與'耗用3:PO已確認且PO未結案且未轉應付的金額'
