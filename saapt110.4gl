   IF cl_null(g_apa.apa61) OR g_apa.apa61=0 THEN
      LET g_apa.apa61 =g_apa.apa60 *g_apa.apa16/100
      LET g_apa.apa61 =cl_digcut(g_apa.apa61,g_azi04)   #MOD-6B0066
   END IF
 
   DISPLAY BY NAME g_apa.apa60f,g_apa.apa60
   DISPLAY BY NAME g_apa.apa61f,g_apa.apa61
 
   DECLARE t110_j_c CURSOR WITH HOLD FOR SELECT apb02,apb11,apb12,apb15,apb08,           #FUN-CB0010 add apb11
                                                apb10,apb13f,apb13,apb14f,apb14
                                               ,apb21,apb22,apb37   #MOD-GB0076 add
                                           FROM apb_file
                                          WHERE apb01 = g_apa.apa01
                                          ORDER BY apb02
 
   LET i = 1

  #--MOD-GB0076 mark start--
  ##FUN-CB0010 add begin---
  #LET l_sql = "SELECT apk03 ",
  #            "  FROM apk_file ",
  #            " WHERE (apk01,apk06) IN (SELECT apk01,max(apk06) ",
  #            "                           FROM apk_file ",
  #            "                          WHERE apk01 = ? ",
  #            "                          GROUP BY apk01)"
  #PREPARE apk_pre FROM l_sql
  #DECLARE apk_cus SCROLL CURSOR FOR apk_pre
  ##FUN-CB0010 add end-----
  #--MOD-GB0076 mark end--
   FOREACH t110_j_c INTO l_apb[i].*
                        ,l_apb21,l_apb22,l_apb37   #MOD-GB0076 add
     #--MOD-GB0076 mark start--
     ##FUN-CB0010 add begin---
     #OPEN apk_cus USING g_apa.apa01
     #FETCH FIRST apk_cus INTO l_apb[i].apb11
     #CLOSE apk_cus
     ##FUN-CB0010 add end-----
     #--MOD-GB0076 mark end--

     #--MOD-GB0076 add start--
     LET l_sql = "SELECT rvb22",
                 "  FROM ",cl_get_target_table(l_apb37,'rvv_file'),",",
                           cl_get_target_table(l_apb37,'rvb_file'),
                 " WHERE rvv01 = '",l_apb21,"' ", 
                 "   AND rvv02 = '",l_apb22,"' ",
                 "   AND rvv04 = rvb01",
                 "   AND rvv05 = rvb02"
     CALL cl_replace_sqldb(l_sql) RETURNING l_sql            
     CALL cl_parse_qry_sql(l_sql,l_apb37) RETURNING l_sql 
     PREPARE rvb_pre2 FROM l_sql
     DECLARE rvb_cus2 SCROLL CURSOR FOR rvb_pre2
     OPEN rvb_cus2
     FETCH FIRST rvb_cus2 INTO l_apb[i].apb11
     CLOSE rvb_cus2 
     #--MOD-GB0076 add end-- 
     LET i = i + 1
   END FOREACH
 
   CALL l_apb.deleteElement(i)
   LET g_rec_b = i - 1
 
   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")
   LET g_success = 'Y'    #CHI-D80028 add
   INPUT ARRAY l_apb WITHOUT DEFAULTS FROM s_apb.*
         ATTRIBUTE(COUNT=g_rec_b,MAXCOUNT=g_max_rec,UNBUFFERED,
                   INSERT ROW=FALSE,DELETE ROW=FALSE,APPEND ROW=FALSE)
 
      BEFORE INPUT
         IF g_rec_b > 0 THEN
            CALL fgl_set_arr_curr(i)
         END IF
 
      BEFORE ROW
         LET i = ARR_CURR()
         CALL cl_show_fld_cont()     #FUN-550037(smin)
         LET l_apk06 = NULL          #FUN-CB0010 add
         LET l_apk06f = NULL         #FUN-CB0010 add

      #FUN-CB0010 add begin-----
      AFTER FIELD  apb11
         IF NOT cl_null(l_apb[i].apb11) THEN
            SELECT COUNT(*) INTO l_cnt
              FROM apk_file
             WHERE apk01 = g_apa.apa01
               AND apk03 = l_apb[i].apb11
            IF l_cnt = 0 THEN
               CALL cl_err('','aap-151',1)
               NEXT FIELD apb11
            END IF

            IF cl_null(l_apk06) OR cl_null(l_apk06f) THEN
               SELECT apk06,apk06f INTO l_apk06,l_apk06f
                 FROM apk_file
                WHERE apk01 = g_apa.apa01
                  AND apk03 = l_apb[i].apb11
            END IF
         ELSE
            LET l_apk06 = NULL 
         END IF
      #FUN-CB0010 add end-----

      AFTER FIELD apb15
         IF l_apb[i].apb14 = 0 THEN   #MOD-5B0255
            LET l_amt = l_apb[i].apb15 * l_apb[i].apb13
            LET l_apb[i].apb14 = l_amt
            LET l_apb[i].apb14 = cl_digcut(l_apb[i].apb14,g_azi04)   #MOD-6B0066
         END IF   #MOD-5B0255
         IF l_apb[i].apb14f = 0 THEN
            LET l_apb[i].apb14f = l_apb[i].apb15 * l_apb[i].apb13f
         END IF
         LET l_apb[i].apb14 = cl_digcut(l_apb[i].apb14,g_azi04)  #MOD-9C0085 add
 
      AFTER FIELD apb13f
         IF l_apb[i].apb14f = 0 THEN
            LET l_apb[i].apb14f = l_apb[i].apb15 * l_apb[i].apb13f
         END IF
         LET l_apb[i].apb14f= cl_digcut(l_apb[i].apb14f,t_azi04)  #MOD-9C0085 add
         IF l_apb[i].apb13 = 0 THEN
            LET l_apb[i].apb13  = l_apb[i].apb13f * g_apa.apa14
         END IF
         IF l_apb[i].apb14 = 0 THEN
            LET l_amt = l_apb[i].apb14f * g_apa.apa14
            LET l_apb[i].apb14  = l_amt
         END IF
         LET l_apb[i].apb14 = cl_digcut(l_apb[i].apb14,g_azi04)  #MOD-9C0085 add
 
      AFTER FIELD apb13
         IF g_apa.apa14 = 1 THEN
            LET l_apb[i].apb13f = l_apb[i].apb13
         END IF
      AFTER FIELD apb14f
         IF l_apb[i].apb14 = 0 THEN
            LET l_amt = l_apb[i].apb14f * g_apa.apa14
            LET l_apb[i].apb14  = l_amt
         END IF 
         #FUN-CB0010 add begin---
         IF l_apb[i].apb14f > l_apk06f THEN
            CALL cl_err('','aap-062',1)
            NEXT FIELD apb14f
         END IF
         #FUN-CB0010 add end-----
         LET l_apb[i].apb14 = cl_digcut(l_apb[i].apb14,g_azi04)  #MOD-9C0085 add

      AFTER FIELD apb14
         IF g_apa.apa14 = 1 THEN
            LET l_apb[i].apb14f = l_apb[i].apb14
         END IF  
         #FUN-CB0010 add begin---
         IF l_apb[i].apb14 > l_apk06 THEN
            CALL cl_err('','aap-062',1)
            NEXT FIELD apb14
         END IF
         #FUN-CB0010 add end-----
         LET l_apb[i].apb14f= cl_digcut(l_apb[i].apb14f,t_azi04)  #MOD-9C0085 add
 
      AFTER ROW
         IF INT_FLAG THEN
            EXIT INPUT
         END IF
         IF l_apb[i].apb02 IS NOT NULL THEN
            UPDATE apb_file SET apb11 = l_apb[i].apb11,     #FUN-CB0010 add apb11
                                apb12 = l_apb[i].apb12,
                                apb15 = l_apb[i].apb15,
                                apb13f= l_apb[i].apb13f,
                                apb13 = l_apb[i].apb13,
                                apb14f= l_apb[i].apb14f,
                                apb14 = l_apb[i].apb14
             WHERE apb01 = g_apa.apa01
               AND apb02 = l_apb[i].apb02
            #CHI-D80028 add begin---
            IF STATUS OR SQLCA.SQLCODE THEN
               LET g_success = 'N'
            END IF
            #CHI-D80028 add end-----
         END IF
 
         IF l_apb[i].apb02 IS NULL AND l_apb[i].apb14 != 0 THEN
            SELECT MAX(apb02)+1 INTO l_apb[i].apb02 FROM apb_file
             WHERE apb01 = g_apa.apa01
            IF l_apb[i].apb02 IS NULL THEN
               LET l_apb[i].apb02 = 1
            END IF
