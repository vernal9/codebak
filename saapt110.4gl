FUNCTION t110_j()
   DEFINE l_wc             STRING    #MOD-720062
   DEFINE i,j              LIKE type_file.num5    #No.FUN-690028 SMALLINT
   DEFINE l_apa60f         LIKE apa_file.apa60f
