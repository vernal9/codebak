  IF (t_tot1 != l_tot1 OR t_tot2 != l_tot2 OR t_tot3 != l_tot3 OR t_tot28 != l_tot28 OR g_apz.apz08='2') THEN #MOD-990177 add OR g_apz.apz08='2'                                            
