   SELECT SUM(apk07+apk08)
     INTO l_amt1
     FROM apk_file
    WHERE apk03 = p_apb11
      AND apk01 = g_apa.apa01
      AND apk02 <> p_apk02
      AND apk171 IN ('23','24','29')
   IF STATUS <> 0 OR l_amt1 IS NULL THEN
