      AND apk02 <> p_apk02
      AND apk171 = 'XX'
      AND apk01 = apa01
      AND apa00 MATCHES "2*"
   IF STATUS <> 0 OR l_amt3 IS NULL THEN
      LET l_amt3 = 0
   END IF

   LET l_amt1 = l_amt1 + l_amt3 + p_apb24

   SELECT COUNT(*) INTO l_cnt
     FROM apk_file
    WHERE apk03 = p_apb11
     #AND apk171 IN ('21','22','25','28','XX')              #TQC-E80007 mark
      AND apk171 IN ('21','22','25','26,','27','28','XX')   #TQC-E80007 add
   IF l_cnt > 0 THEN
     ##原發票金額
      SELECT SUM(apk07+apk08)
        INTO l_amt2
        FROM apk_file
       WHERE apk03 = p_apb11
        #AND apk171 IN ('21','22','25','28')             #TQC-E80007 mark
         AND apk171 IN ('21','22','25','26','27','28')   #TQC-E80007 add
      IF STATUS <> 0 OR l_amt2 IS NULL THEN
         LET l_amt2 = 0
      END IF

      SELECT SUM(apk07+apk08)
        INTO l_amt4
        FROM apk_file,apa_file
       WHERE apk03 = p_apb11
         AND apk171 = 'XX'
         AND apk01 = apa01
         AND apa00 MATCHES "1*"
      IF STATUS <> 0 OR l_amt4 IS NULL THEN
         LET l_amt4 = 0
      END IF

      LET l_amt2 = l_amt2 + l_amt4
   ELSE
      SELECT SUM(amd07+amd08)
        INTO l_amt2
        FROM amd_file
       WHERE amd03 = p_apb11
