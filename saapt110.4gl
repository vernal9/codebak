                RETURNING bamt1,bamt2,bamt3,bamt4,bamt5,bamt6
          #MOD-A30113---modify---end---
        END IF
        IF cl_null(bamt3) THEN LET bamt3 = 0 END IF
        IF cl_null(bamt4) THEN LET bamt4 = 0 END IF
        #要確認目前是和耗用3還是耗用4重疊
        LET l_cnt = 0
       #FUN-A60056--mod--str--
