   END FOREACH   #MOD-790033
 
   #有匯差的情況                                                                                         
   SELECT MAX(api03)+1,SUM(api05f),SUM(api05)                                                                                    
     INTO l_diff_api03,l_diff_api05f,l_diff_api05                                                                                
