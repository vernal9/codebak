#確認、作廢時新增記錄到進項作廢折讓發票檔(ave_file)
FUNCTION t210_ins_ave(p_apa01,p_void)
   DEFINE p_apa01  LIKE apa_file.apa01
   DEFINE p_void   LIKE type_file.chr1
   DEFINE l_sql    STRING
   DEFINE l_apk    RECORD
            apk01    LIKE apk_file.apk01,
            apk02    LIKE apk_file.apk02,
            apk03    LIKE apk_file.apk03,
            apk04    LIKE apk_file.apk04,
            apk05    LIKE apk_file.apk05,
            apk11    LIKE apk_file.apk11,
            apk08f   LIKE apk_file.apk08f,
            apk08    LIKE apk_file.apk08,
            apk07f   LIKE apk_file.apk07f,
            apk07    LIKE apk_file.apk07,
            apk06f   LIKE apk_file.apk06f,
            apk06    LIKE apk_file.apk06,
            apk22    LIKE apk_file.apk22,
            apk32    LIKE apk_file.apk32,
            ave12    LIKE type_file.chr1,
            apkacti  LIKE apk_file.apkacti,
            apkdate  LIKE apk_file.apkdate,
            apkgrup  LIKE apk_file.apkgrup,
            apkmodu  LIKE apk_file.apkmodu,
            apkorig  LIKE apk_file.apkorig,
            apkoriu  LIKE apk_file.apkoriu,
            apkuser  LIKE apk_file.apkuser,
            apklegal LIKE apk_file.apklegal
                     END RECORD
   #FUN-I80008 18/08/09 By TSD.Yuwei add -----(S)
   DEFINE l_apa05    LIKE apa_file.apa05
   DEFINE l_cnt      LIKE type_file.num5 
   
   LET l_apa05 = NULL 
   SELECT apa05 INTO l_apa05 FROM apa_file WHERE apa01 = p_apa01  
    
   LET l_cnt = 0 
   SELECT COUNT(1) INTO l_cnt FROM DSC_TRFDB.avd_file 
    WHERE avd01 = l_apa05 
   IF l_cnt = 0 THEN 
      CALL s_errmsg('','','ins ave_file','aap1018',1) #請款廠商不存在廠商電子發票基本資料檔中
      RETURN
   END IF
   #FUN-I80008 18/08/09 By TSD.Yuwei add -----(E)

   LET l_sql = "SELECT apk01,apk02,apk03,apk04,apk05,apk11,apk08f,apk08,apk07f,apk07,apk06f,apk06,apk22,apk32,'', ",
               "       apkacti,apkdate,apkgrup,apkmodu,apkorig,apkoriu,apkuser,apklegal ",
               "  FROM apk_file ",
               " WHERE apk01 = '",p_apa01 CLIPPED,"' "
   PREPARE t210_ins_ave_pre FROM l_sql
   DECLARE t210_ins_ave_cus CURSOR FOR t210_ins_ave_pre

   FOREACH t210_ins_ave_cus INTO l_apk.*
      IF SQLCA.sqlcode THEN
         CALL cl_err('FOREACH t210_ins_ave_cus fail:',SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF
      #FUN-I80008 18/08/09 By TSD.Yuwei add -----(S) 
      LET l_cnt = 0 
      SELECT COUNT(1) INTO l_cnt 
        FROM gec_file 
       WHERE gec05 = '7' 
         AND gec011 = '1' 
         AND gec01 = l_apk.apk11 
      
      IF l_cnt = 0 THEN 
         CONTINUE FOREACH 
      END IF 
      #FUN-I80008 18/08/09 By TSD.Yuwei add -----(E)
      LET l_apk.ave12 = 'N'
      
      INSERT INTO DSC_TRFDB.ave_file VALUES(l_apk.*)
      IF SQLCA.sqlcode THEN
         CALL cl_err('ins ave_file fail:',SQLCA.sqlcode,1)
         LET g_success = 'N'
         EXIT FOREACH
      END IF
   END FOREACH
   #作廢
   IF p_void = 'Y' THEN
      FOREACH t210_ins_ave_cus INTO l_apk.*
         IF SQLCA.sqlcode THEN
            CALL cl_err('FOREACH t210_ins_ave_cus fail:',SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
         SELECT MAX(ave02)+1 INTO l_apk.apk02
          FROM DSC_TRFDB.ave_file
          WHERE ave01 = p_apa01
         
         LET l_apk.ave12 = 'Y'
         
         INSERT INTO DSC_TRFDB.ave_file VALUES(l_apk.*)
         IF SQLCA.sqlcode THEN
            CALL cl_err('ins ave_file fail:',SQLCA.sqlcode,1)
            LET g_success = 'N'
            EXIT FOREACH
         END IF
      END FOREACH
   END IF
   RETURN

END FUNCTION

