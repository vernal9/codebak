            DISPLAY BY NAME g_apa.apa51
            CALL t110_apa51()
            NEXT FIELD apa51      
  
         WHEN INFIELD(apa52) 
            CALL cl_init_qry_var()
            LET g_qryparam.form ="q_aag02" 
            LET g_qryparam.default1 = g_apa.apa52
            LET g_qryparam.arg1 = g_bookno1 
