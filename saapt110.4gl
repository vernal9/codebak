                      IF STATUS OR SQLCA.SQLCODE THEN
                         CALL cl_err3("upd","apcfile", g_apa.apa01,g_apv[i].apv05,SQLCA.sqlcode,"","",1)  
                         LET g_success='N'
                         IF p_cmd = 'u' THEN
                             LET g_apv[i].*=g_apv_t.*
