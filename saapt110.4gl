  END FOREACH

  IF g_aza.aza17 = l_apk12 AND g_cnt = 1 THEN
     LET l_apa32f = l_tot2
  ELSE
     LET l_apa32f = cl_digcut(l_tot2f,t_azi04)
  END IF

  RETURN l_apa32,l_apa32f

END FUNCTION
