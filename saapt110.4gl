    FOR i = 1 TO g_api.getLength() #No.MOD-480131
      IF cl_null(g_api[i].api04) AND cl_null(g_api[i].api041) THEN    #No.TQC-7A0095 
         CONTINUE FOR 
      END IF
      IF g_api[i].api05f = 0 THEN CONTINUE FOR END IF
