FUNCTION t110_amt_chk()
   DEFINE l_apa31     LIKE apa_file.apa31   #帳款金額
   DEFINE l_apa32     LIKE apa_file.apa32   #分錄金額
   DEFINE l_apa33     LIKE apa_file.apa33   #MOD-E40178 add
   DEFINE l_npq07     LIKE npq_file.npq07 
   DEFINE l_npq07_2   LIKE npq_file.npq07   #借方的匯損金額
  #DEFINE l_apb10_sum LIKE type_file.num10  #MOD-E80139 add   #MOD-EC0142 mark
   DEFINE l_apb10_sum LIKE apb_file.apb10                     #MOD-EC0142 add
   DEFINE l_apb10_sum_2 LIKE apb_file.apb10                   #MOD-F60105 add
   DEFINE l_apb10_aag42 LIKE apb_file.apb10                   #MOD-G10082 add
   DEFINE l_flag      LIKE type_file.chr1   #MOD-F60021 add
   DEFINE l_t1        LIKE apy_file.apyslip   #MOD-G30005 add
   DEFINE l_apb10_1   LIKE type_file.num5   #MOD-G60017 add

   LET l_apa31 = 0
   LET l_apa32 = 0
   LET l_flag = 'Y'   #MOD-F60021 add

  #MOD-G70072---add---str--
   IF g_aza.aza63 = 'Y' THEN
      SELECT SUM(npq07f) INTO l_apa32 FROM npq_file
       WHERE npq01 = g_apa.apa01
         AND npqsys='AP'
         AND npqtype = '1'
   ELSE
      SELECT SUM(npq07f) INTO l_apa32 FROM npq_file
       WHERE npq01 = g_apa.apa01
         AND npqsys='AP'
         AND npqtype = '0'
   END IF
   IF l_apa32 = 0 THEN
      RETURN 'N'
   END IF
  #MOD-G70072---add---end--

   IF g_aza.aza63 = 'Y' THEN    
    SELECT SUM(npq07) INTO l_apa32 FROM npq_file
     WHERE npq01 = g_apa.apa01 
       AND npqsys='AP'
       AND npq06 = "2"
       AND npqtype = '1'
   ELSE
    SELECT SUM(npq07) INTO l_apa32 FROM npq_file
     WHERE npq01 = g_apa.apa01 
       AND npqsys='AP'
       AND npq06 = "2"
       AND npqtype = '0'
   END IF

   #抓取分錄金額時,需扣除手續費(aph03=45AB)金額
   LET l_npq07 = 0
  #MOD-G80040---mark---str--
  #SELECT SUM(npq07) INTO l_npq07 FROM npq_file
  # WHERE npq01 = g_apa.apa01 
  #   AND npqsys= 'AP'
  #   AND npq06 = '1'
  #   AND npq03 IN (SELECT aph04 FROM aph_file
  #                  WHERE aph01= g_apa.apa01
  #                   AND aph03 IN ('3','4','5','A','B'))   #MOD-G30162 mod 增加'3'
  #MOD-G80040---mark---end--
  #MOD-G80040---add---str--
   #如果手續費的科目與單身科目相同,則會扣到非手續費的金額,故調整SQL
   SELECT SUM(npq07) INTO l_npq07
     FROM npq_file,aph_file
    WHERE npq01 = g_apa.apa01 
      AND npqsys= 'AP'
      AND npq06 = '1'
      AND npq01 = aph01
      AND npq03 = aph04
      AND aph03 IN ('3','4','5','A','B')      
      AND npq07 = ABS(aph05)
  #MOD-G80040---add---end--
   IF cl_null(l_npq07) THEN LET l_npq07=0 END IF
   LET l_apa32 = l_apa32 - l_npq07
  
   #增加扣除 apv_file 沖帳的 DIFF 金額
   LET l_npq07_2 = 0
   SELECT SUM(apv04) INTO l_npq07_2
     FROM apv_file 
    WHERE apv01 = g_apa.apa01 
     #AND (apv03 = 'DIFF' OR apv04 < 0)   #MOD-E80140 mark
      AND apv03 = 'DIFF' AND apv04 < 0    #MOD-E80140 add

   IF cl_null(l_npq07_2) THEN LET l_npq07_2=0 END IF
   IF l_npq07_2 < 0 THEN LET l_npq07_2 = l_npq07_2 * -1 END IF
   LET l_apa32 = l_apa32 - l_npq07_2 

   #抓取分錄金額時,應扣除暫估差異<0的部份
   LET l_npq07 = 0
   SELECT SUM(api05) INTO l_npq07 FROM api_file
    WHERE api01 = g_apa.apa01 
    #AND (api26 = 'DIFF' OR api05 < 0)   #MOD-EC0042 mark
    #AND api26 = 'DIFF'                  #MOD-EC0042 add   #MOD-F50083 mark
    #AND (api26 = 'DIFF'                                   #MOD-F50083 add   #MOD-F50128 mark
     AND (api05 < 0                                                          #MOD-F50128 add
      OR (api26 = 'DIFF2' AND api05 < 0))                  #MOD-F50083 add
   IF cl_null(l_npq07) THEN LET l_npq07=0 END IF
   IF l_npq07 < 0 THEN
      LET l_apa32 = l_apa32 + l_npq07
   END IF

  #IF NOT (l_npq07 < 0) THEN   #MOD-F50128 add  #MOD-F60105 mark 
   #--MOD-E80139 add start--
   #l_apa32 當 apa00 為 11 時,應扣除倉退金額
   IF g_apa.apa00 = '11' THEN                              #MOD-F10090 mark                           #MOD-F60105 undo mark
  #IF g_apa.apa00 = '11' AND (g_apa.apa51 = 'STOCK' OR     #MOD-F10090 add                            #MOD-F60105 mark
  #  #g_apa.apa51 = 'MISC' OR g_apa.apa51 = 'UNAP') THEN   #MOD-F10090 add         #MOD-F50128 mark   #MOD-F60105 mark
  #   g_apa.apa51 = 'MISC' OR g_apa.apa51 = 'UNAP' OR cl_null(g_apa.apa51)) THEN   #MOD-F50128 add    #MOD-F60105 mark
     #MOD-F60105---add---str--
      SELECT SUM(apb10) INTO l_apb10_sum_2
        FROM apb_file
       WHERE apb01 = g_apa.apa01
         AND (apb34 IS NULL OR apb34 = 'N') 
      IF cl_null(l_apb10_sum_2) THEN LET l_apb10_sum_2 = 0 END IF
      IF l_apb10_sum_2 < 0 OR (l_apb10_sum_2 > 0 AND 
         (g_apa.apa51 = 'STOCK' OR     
          g_apa.apa51 = 'MISC' OR g_apa.apa51 = 'UNAP' OR cl_null(g_apa.apa51))) THEN 
     #MOD-F60105---add---end--
         SELECT SUM(apb10) INTO l_apb10_sum
           FROM apb_file
          WHERE apb01 = g_apa.apa01
            AND apb29 = '3'
            AND apb34 <> 'Y'   #MOD-F60105 add
        #MOD-EB0082---add---str--
         IF cl_null(l_apb10_sum) THEN 
            LET l_apb10_sum = 0 
         END IF
        #MOD-EB0082---add---end--
         IF l_apb10_sum < 0 THEN
            LET l_apb10_sum = l_apb10_sum * -1
         END IF
        #MOD-G10082---add---str--
         IF g_aza.aza26 = '2' THEN
            #單身科目
            LET l_apb10_aag42 = 0
            SELECT SUM(apb10) INTO l_apb10_aag42
              FROM apb_file,aag_file
             WHERE apb01 =  g_apa.apa01
               AND apb29 = '3'
               AND apb34 <> 'Y'
               AND apb25 = aag01
               AND aag00 = g_bookno1 
               AND aag06 = '1'
               AND aag42 = 'Y'
            IF cl_null(l_apb10_aag42) THEN LET l_apb10_aag42 = 0 END IF
            IF l_apb10_aag42 < 0 THEN
               LET l_apb10_aag42 = l_apb10_aag42 * -1
            END IF
            LET l_apb10_sum = l_apb10_sum - l_apb10_aag42
         END IF 
        #MOD-G10082---add---end--
         #MOD-G60017---add---str--
         LET l_apb10_1 = 0
         SELECT COUNT(*) INTO l_apb10_1
           FROM apb_file
          WHERE apb01 = g_apa.apa01
            AND (apb34 IS NULL OR apb34 = 'N')
            AND apb29 = '1'
         IF cl_null(l_apb10_1) THEN LET l_apb10_1 = 0 END IF
         IF l_apb10_sum_2 < 0 AND l_apb10_1 > 0 THEN 
            LET l_apb10_sum = l_apb10_sum_2 * -1 
         END IF
         #MOD-G60017---add---end--
         LET l_apa32 = l_apa32 - l_apb10_sum 
      END IF                    #MOD-F60105 add
   END IF
   #--MOD-E80139 add end--
  #END IF    #MOD-F50128 add    #MOD-F60105 mark

  #MOD-G30005---add---str--
   LET l_t1 = s_get_doc_no(g_apa.apa01)
   SELECT apydmy6 INTO g_apy.apydmy6 FROM apy_file WHERE apyslip = l_t1
   IF g_aza.aza26 = '2' AND g_apy.apydmy6 = 'Y' THEN
      IF l_apa32 < 0 THEN
         LET l_apa32 = l_apa32 * -1
      END IF
   END IF
  #MOD-G30005---add---end--

   #帳款金額
   SELECT apa31+apa32+nvl(apa37,0)-apa60 INTO l_apa31  
     FROM apa_file  
    WHERE apa01 = g_apa.apa01   

   #--MOD-E40178 add test start--
   LET l_apa33 = 0
   SELECT apa33 INTO l_apa33
     FROM apa_file
    WHERE apa01 = g_apa.apa01
      AND apa56 = '2'   #MOD-E50066 add
      AND apa33 < 0     #MOD-EB0082 add
   IF cl_null(l_apa33) THEN
      LET l_apa33 = 0
   END IF
  #IF l_apa33 < 0 THEN   #MOD-EB0082 mark
      LET l_apa33 = l_apa33 * -1
  #END IF   #MOD-EB0082 mark
   #--MOD-E40178 add test end--
   
#180702 add by ruby --s--
  LET l_apb10_1 = 0
  select count(*) into l_apb10_1 from apa_file where apa01=g_apa.apa01 
    and apa01 in (select apb01 from apb_file where apb29=1 and apb34='Y')
    and apa01 in (select apb01 from apb_file where apb29=1 and apb34='N')
    and apa01 in (select apb01 from apb_file where apb29=3 and apb34='N')
  IF l_apb10_1>0 THEN
  select sum(abs(apb10))+apa32 into l_apa31 
    from apa_file inner join apb_file on apa01=apb01
    where apb01=g_apa.apa01 and apb10>0 group by apa32  
    
  SELECT SUM(npq07) into l_apa32 FROM npq_file
       WHERE npq01=g_apa.apa01 
       AND npqsys='AP'
       AND npq06 = '2'
       AND npqtype = '0'

   #230105 add by ruby --s--
   LET l_npq07 = 0
   SELECT SUM(api05) INTO l_npq07 FROM api_file
    WHERE api01 = g_apa.apa01 
     #AND (api05 < 0                             #250625 mark by ruby
     AND ((api26 = 'DIFF' AND api05 < 0)         #250625 add by ruby                          
      OR (api26 = 'DIFF2' AND api05 < 0))                  
   IF cl_null(l_npq07) THEN LET l_npq07=0 END IF
   IF l_npq07 < 0 THEN
      LET l_apa32 = l_apa32 + l_npq07
   END IF
