    IF cl_null(l_cnt01) THEN LET l_cnt01 = 0 END IF
    #IF l_cnt01 > 0 THEN                                                        #210827 mark by ruby
    #   CALL cl_err('','aap-349',1)                                             #210827 mark by ruby
    #   RETURN                                                                  #210827 mark by ruby
    #END IF                                                                     #210827 mark by ruby

    LET g_sql = "SELECT azp01 FROM azp_file",
                " WHERE azp03 = (SELECT azw09 FROM azw_file WHERE azw01 = '",g_plant CLIPPED,"')"

    PREPARE zp_pre_1 FROM g_sql
    DECLARE zp_curs1_1 CURSOR FOR zp_pre_1

    FOREACH zp_curs1_1 INTO g_plant_new
       IF STATUS THEN
